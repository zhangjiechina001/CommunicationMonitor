QTableView#IDC_tableView_currentResult {

    /*alternate-background-color:rgb(67,150,202);*/
    gridline-color:rgb(67,150,202);
    font:25px;
    /*font-weight:bold;*/
    color:black;
    font-family:"Microsoft YaHei";
}

QListWidget#listWidget {
    show-decoration-selected: 1; /* make the selection span the entire width of the view */
    border:0px solid white;
    color:#001558;
    margin:0px,0px,0px,0px;
    padding-top:40px;
    border-image:url(:/Config/listwidget.png);
}

QListWidget#listWidget ::item
{
    padding-top:10px;
    padding-bottom:5px;
    padding-left:5px;
    padding-right:10px;
}

QListWidget#listWidget ::item:selected {
    background:rgb(255, 255, 255); /*改变背景色*/
    show-decoration-selected: 1
}

QTextEdit{
    background:rgb(255, 255, 255); /*改变背景色*/
}

QTextEdit#IDC_textEdit_CurResult {
    border:0,0,0,0;
}

QPushButton#IDC_Btn_StartMeasure
{
    background-color:rgba(255, 255, 255,0);
    border-style:outset;  
    border-width:0px;  
    border-color:rgb(10,45,110); 
    border-radius:0px;  
    padding:4px;
    qproperty-iconSize: 30px 40px;
}

/*鼠标不可用时的效果*/
QPushButton#IDC_Btn_StartMeasure:disabled
{
    color: rgb(67, 150, 202);
    background-color:white;
    border-style:inset;
    padding-left:0px;
    padding-top:0px;
    qproperty-iconSize: 30px 40px;
}

/*鼠标不可用时的效果*/
QPushButton#IDC_Btn_Stop:pressed
{
    background-color:white;
    border-style:inset;
    padding-left:0px;
    padding-top:0px;
}

QPushButton#IDC_Btn_Stop
{
    background-color:rgba(255, 255, 255,0);
    border-style:outset;
