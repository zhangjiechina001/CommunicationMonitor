QPushButton#IDC_Btn_SaveInput
{
    background-color:rgb(67, 150, 202);
    color:rgb(0, 21, 88);
    border-style:outset;
    border-width:0px;
    border-color:rgb(10,45,110);
    border-radius:10px;
    padding:4px;
}

QCheckBox#IDC_Chk_AutoRemark {
    spacing:5px;
}

QCheckBox#IDC_Chk_AutoRemark::indicator {
      width: 25px;
      height: 25px;
}

/*鼠标按压时的效果*/
QPushButton#IDC_Btn_SaveInput:disabled
{
    color:gray;
    background-color:rgb(183, 192, 200); /*改变背景色*/
    border-style:inset;/*改变边框风格*/
    padding-left:0px;
    padding-top:0px;
}
/*鼠标按压时的效果*/
QPushButton#IDC_Btn_SaveInput:pressed
{
    color:white;
    background-color:rgb(0, 21, 88); /*改变背景色*/
    border-style:inset;/*改变边框风格*/
    padding-left:0px;
    padding-top:0px;
}

